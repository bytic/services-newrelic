    "bytic/http": "^0.9|^1.0",
    "monolog/monolog": "^1.2|^2.0",
    "vlucas/phpdotenv": "^5.0",
    "phpunit/phpunit": "^7.0|^8.0|^9.0",
    "mockery/mockery": "^1.0"
  }
}
