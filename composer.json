{
  "name": "bytic/newrelic",
  "description": "Description Composer Package for NewRelic Wrapper",
  "homepage": "https://github.com/ByTIC/services-newrelic",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
