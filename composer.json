{
  "name": "bytic/newrelic",
  "description": "Description Composer Package for NewRelic Wrapper",
  "homepage": "https://github.com/ByTIC/services-newrelic",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\NewRelic\\": "src/"
    },
    "classmap": [
      "legacy"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\NewRelic\\Tests\\": "tests/src"
    }
  },
  "require": {
    "php": "^7.2|^8.0",
    "psr/http-message": "^1.0|^2.0"
  },
  "require-dev": {
    "bytic/container": "^0.9|^1.0|^2.0",
    "bytic/http": "^0.9|^1.0|^2.0",
    "monolog/monolog": "^1.2|^2.0|^3.0",
    "vlucas/phpdotenv": "^5.0",
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
